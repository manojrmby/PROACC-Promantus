    async function ReloadSAPTables(ID, URL) {   
        $("#load").css("display", "block");
        $.ajax({
            //cache: false,
            type: "POST",
            //async: false,
            url: URL,
            data: { Ins: ID },
            dataType: "json",
            //contentType: "application/json; charset=utf-8",
            success: function (response) {
                if (response == 'success') {
                    $("#load").css("display", "none");
                    Notiflix.Report.Success(
                        'Alert',
                        'Data pulled from sever successfully',
                        'Close',
                        function () {
                            location.reload();
                        },
                    );                    
                }                
            },
            error: function (e) {
                alert(e.responseText);
            }
        });
    };
</script>