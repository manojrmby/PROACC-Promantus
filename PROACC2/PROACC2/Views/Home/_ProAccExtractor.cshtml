    function BreakdownbyLob(ID,sys_LandScape,URL) {
        $.ajax({
            cache: false,
            type: "GET",
            async: false,
            url: URL,
            data: { Ins: ID,sys_LandScape:sys_LandScape },
            success: function (Data) {

                if (Data._List != null) {

                    var Output = Data._List;
                    var inpName = [];
                    var inpVal = [];
                    var inp_percent = [];
                    var C = 0;
                    for (var i = 0; i < Output.length; i++) {
                        inpName.push(Output[i].Name);
                        //inpVal.push(Output[i]._Value);
                        inp_percent.push(Output[i].percent);
                    }

                    labels = inpName;
                    data_ = inp_percent;
                    let Data_ = ConvertToJSON(data_, labels);

                    var high_usage = 0;
                    var low_usage = 0;
                    for (var i = 0; i < Data_.length; i++) {
                        if (Data_[i].count > 10) {
                            high_usage += 1;
                        }
                        else {
                            low_usage += 1;
                        }
                    }
                    $("#Breakdown_Lob").empty();
                    var summ = '<h6>Summary</h6>';
                    summ += '<div><a>Total no. of modules: <b>' + Data_.length + '</b></a></div>';
                    summ += '<div><a>No. of modules with high usage: <b>' + high_usage + '</b></a></div>';
                    summ += '<div><a>No. of modules with low usage: <b>' + low_usage + '</b></a></div>';
                    $("#Breakdown_Lob").append(summ);

                    BreakdownbyLob_Bar_Chart(Data_);
                }
            },
            error: function () {
                alert("Error while Fetching Breakdown by Lob data");
            }
        }).done(function (response) {
            //chart.data = response;

        });
    }

