    $("#btnPwdUpdate").click(function () {
        var URL = '@Url.Action("UpdatePassword", "Login")';
        var OLD_Pwd_URL = '@Url.Action("OldPasswordcheck", "Login")';
        //PwdUpdate(URL, OLD_Pwd_URL);
        var _oldpassword1 = $("#txtOldPassword").val().trim();
        var _newPassword = $("#txtNewPassword").val().trim();
        var _cnfPassword = $("#txtCnfPassword").val().trim();

        var model = {
            Password: _newPassword
        }

        var _Pwd = _Newpwd = _Cnfpwd = false;
        if (_oldpassword1 == "") {
            _Pwd = true;
            $("#lblOldPassword").html("Please enter the Old Password").show();
            $("#txtOldPassword").val("");
        }
        else if (oldpasswordcheck(OLD_Pwd_URL) == false) {
            _Pwd = true;
            $("#lblOldPassword").html("Please enter the Correct Password").show();
            $("#txtOldPassword").val("");
            $("#txtOldPassword").val("");
            $("#txtNewPassword").val("");
            $("#txtCnfPassword").val("");
        }
        else {
            $("#lblOldPassword").html("").show();
        }

        if (_newPassword == "") {
            _Newpwd = true;
            $("#txtNewPassword").val("");
            $("#lblNewPassword").html("Please enter the New Password").show();

        }
        else if (TaskPwd() == false) {
            _Newpwd = true;
            $("#txtNewPassword").val("");
            $("#txtCnfPassword").val("");
            //  $("#lblNewPassword").html("Password should contain one character, Special char and number").show();
        }
        else if (_oldpassword1 != "" && _newPassword != "") {
            if (oldnewpassword() == false) {
                _Newpwd = true;
                $("#txtNewPassword").val("");
                $("#lblNewPassword").html("Choose another password except old").show();
            }
        }
        else {
            $("#lblNewPassword").html("").show();
        }

        if (_cnfPassword == "") {
            _Cnfpwd = true;
            $("#lblCnfPassword").html("Please enter the Confirm Password").show();
        }
        else if (cnfpassword() == false) {
            _Cnfpwd = true;
            $("#txtCnfPassword").val("");
            $("#lblCnfPassword").html("Password Does Not Match Kindly Check").show();
        }
        else {
            $("#lblCnfPassword").html("").show();
        }
        if (_Newpwd == false && _Pwd == false && _Cnfpwd == false) {
            $.ajax({
                type: "POST",
                url: URL,
                data: model,
                success: function (response) {
                    if (response == "success") {
                        Notiflix.Notify.Success('Password <Strong>updated </strong>successfully..!', { cssAnimationStyle: 'zoom', cssAnimationDuration: 500, });
                        ResetUpdtPwd();
                        location.href = $("#RedirectToHome").val();                        
                    }
                },
                failure: function (response) {
                    alert(response.responseText);
                },
                error: function (response) {
                    alert(response.responseText);
                }
            });
        }
    });



