        /***************Camera New Design *******************/

        .hoverimage {
            position: absolute;
            z-index: 1;
            left: 76%;
            border-radius: 50%;
            height: 31px;
            box-shadow: 5px 5px 5px grey;
            /* margin-top: -12%; */
            margin-top: 104px;
        }

        #hoverCamera {
            z-index: 1;
            color: white;
            position: relative;
            left: 1%;
            top: -9%;
            font-size: 2.97em;
        }

        /*#camera {
            position: relative;
            z-index: 1;
            top: 20%;
            pointer-events: inherit;
            cursor: pointer;
            font-size: 1.99em;
            left: 19%;
            border-radius: 50%;
            color: darkslategrey;
        }*/

        .fa-camera:hover {
            color: mediumaquamarine;
            transition: 0.5s ease;
        }
        /***************Camera New Design End*******************/

        li.upload {
            position: relative;
            width: 33px;
            height: 23px;
            justify-content: center;
            display: inline-flex;
            z-index: 1;
            cursor: pointer;
            left: 56%;
        }

        #DrpRefresh {
            margin-top: 290px;
            position: fixed;
            background-color: white;
            border-radius: 3%;
            margin-left: -5%;
            height: 233px;
            display: none;
            background-color: #E9EEF3;
            border: 1px solid black;
            border-top-color: white;
        }


        #close {
            position: absolute;
            left: 99%;
            top: 0%;
            font-size: 1.35em;
            cursor: pointer;
        }

            #close:hover {
                transition: 0.5s ease;
                color: red;
                font-size: 1.8em;
                margin-left: -3%;
            }

        .hovereffect {
            position: relative;
            left: 5%;
            height: 121px;
        }

        #UserName {
            position: relative;
            left: 16%;
            top: 5%;
            font-weight: bolder;
            background-color: #E9EEF3;
        }
        /********************New Camera Setting Ended***************/
        /*  LOADING SPINNER  */
        .loadingDiv-parent {
            position: fixed;
            width: 100%;
            left: 0;
            right: 0;
            top: 0;
            bottom: 0;
            background-color: rgba(255,255,255,0.7);
