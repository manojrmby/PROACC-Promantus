            /*display: none;*/
        }
        .Processing {
            position: fixed;
            width: 100%;
            left: 0;
            right: 0;
            top: 0;
            bottom: 0;
            background-color: rgba(255,255,255,0.7);
            z-index: 9999;
            /*display: none;*/
        }
        .loadingDiv {
            position: fixed;
            text-align: center;
            top: 40%;
            left: 45%;
            z-index: 999999;
        }

            .loadingDiv .loading-image {
                display: block;
                /* margin: 25px 75px;*/
                width: 50%;
            }

        td {
            text-overflow: ellipsis;
            overflow: hidden;
            white-space: nowrap;
        }

        /*more less*/


        .morecontent span {
            display: none;
        }





        /*more less*/

        .arrow-top .dropdown-item:hover {
            background-color: var(--primary) !important;
            color: #fff !important;
            /*border-radius:10px;*/
        }

        .arrow-top {
            margin-top: 10px !important;
        }

            .arrow-top:after {
                content: " ";
                position: absolute;
                right: 0px;
                top: -10px;
                border-top: none;
                border-right: 10px solid transparent;
                border-left: 10px solid transparent;
                border-bottom: 10px solid rgba(255,255,255,0.7);
            }
    </style>


