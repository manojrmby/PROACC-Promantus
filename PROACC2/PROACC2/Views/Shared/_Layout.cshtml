<div id="UpdatePwd" class="modal fade" role="dialog" data-backdrop="static" data-keyboard="false">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title text-center w-100 semibold">Update Password</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close" id="btnUpdtPWdClose">
                    <span aria-hidden="true">x</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="row bg-white">
                    <div class="ibox w-100">
                        <div class="ibox-body">
                            <form>
                                <div class="form-group">
                                    <label class="control-label col-md-5 required">Old Password<span id="Oldpwd" style="color:red">*</span></label>
                                    <div class="col-md-10">
                                        <input type="password" class="form-control" id="txtOldPassword" style="border: 2px solid #ccc !important" />
                                    </div>
                                    <div>
                                        &nbsp;&nbsp;&nbsp;<span class="Class_span" id="lblOldPassword" style="color:red">&nbsp;</span>
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label class="control-label col-md-5 required">New Password<span id="Newpwd" style="color:red">*</span></label>
                                    <div class="col-md-10">
                                        <input type="password" class="form-control" id="txtNewPassword" maxlength="8" style="border: 2px solid #ccc !important" />
                                    </div>
                                    <div>
                                        &nbsp;&nbsp;&nbsp;<span class="Class_span" id="lblNewPassword" style="color:red">&nbsp;</span>
                                    </div>
                                </div>

