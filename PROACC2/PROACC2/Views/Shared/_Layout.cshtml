<script>

     $(document).ready(function () {
         var User = '@Session["UserType"]';
        if (User == 'Consultant' || User == 'Customer') {
            $.ajax({
                type: 'Get',
                url: '@Url.Action("__ViewMessage", "Home")',
                async: false,
                success: function (response) {
                    if (response.length > 0) {
                        $('.notify-signal').show();
                    }
                    else {
                        $('.notify-signal').hide();
                    }
                }
            });
        }
        else {
            //$('.notify-signal').show();
            $('.notify-signal').hide();
        }
    });

    $('.NotifyMessage').click(function () {
        var User = '@Session["UserType"]';
        if (User == 'Consultant' || User == 'Customer') {
            $.ajax({
                type: 'Get',
                url: '@Url.Action("__ViewMessage", "Home")',
                async: false,
                success: function (response) {
                    $('.MessageShow').empty();
                    var aa1 = '<a>';
                    var Count = "";
                    for (var i = 0; i < response.length; i++) {
                         Count= '<div class="dropdown-header bg-light"><strong>You have ' + response[i].CountMessgae + ' messages</strong></div>';
                    }
                    var dd = '<div class="message">';
                    if (response.length > 0) {
                        $.each(response, function (index, val) {
                            var ConsUserName = val.ConsUserName;
                            var PmName = val.PmName;
                            var IssueName = val.IssueName;
                            var Cre_on = val.Createdon;

                            var aa = '<a class="dropdown-item notifyread" href="#" onclick="redirect()" id=' + val.Id + '>';
                            var bb = '<div><small class="text-muted">' + ConsUserName + '</small><small class="text-muted float-right mt-1">' + Cre_on + ' </small></div>';

                            bb += '<div class="text-truncate font-weight-bold"><span class="text-danger">!</span> Important message</div>';
                            bb += '<div class="small text-muted text-truncate">A New Issue <strong>' + IssueName + '</strong> Is Being Issued By <strong>' + PmName + '</strong> To You </div>';

                            aa = aa + bb + '</a>';
                            dd = dd + aa + '</div>';
                        });

                        //aa1 = aa1 + dd + '</a>';
                        Count = Count + dd + '</div>';
                        $('.MessageShow').append(Count);
                        $('.notify-signal').show();
                    }
                }
            });
        }
    });

    function redirect() {
        
        var id = $('.MessageShow').find('.notifyread').attr('id');
        $.ajax({
            type: 'Post',
            url: '@Url.Action("DeleteMessage","Home")',
